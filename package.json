{
    "name": "dashooks",
    "version": "1.0.2",
    "description": "useful collections of hooks that I've been copying from project to project. Now everyone can use it.",
    "private": false,
    "repository": "https://github.com/dr1tch/dashooks",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "scripts": {
        "test": "vitest run",
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
        "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
        "lint": "tsc && pnpm run lint:format && pnpm run lint:fix ",
        "ci": "pnpm run lint &&  pnpm run build",
        "release": "pnpm run lint &&  pnpm run build && changeset version && changeset publish"
    },
    "keywords": [],
    "author": "Youssouf Kacemi",
    "license": "MIT",
    "devDependencies": {
        "@changesets/cli": "^2.26.1",
        "@types/node": "^18.16.2",
        "@types/react": "^18.2.0",
        "@types/uuid": "^9.0.1",
        "@typescript-eslint/eslint-plugin": "^5.59.1",
        "@typescript-eslint/parser": "^5.59.1",
        "eslint": "^8.39.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "firebase": "^9.21.0",
        "husky": "^8.0.0",
        "lint-staged": "^13.2.2",
        "pre-commit": "^1.2.2",
        "prettier": "^2.8.8",
        "react": "^18.2.0",
        "tsup": "^6.7.0",
        "typescript": "^5.0.4",
        "uuid": "^9.0.0",
        "vitest": "^0.30.1"
    },
    "pre-commit": "lint",
    "publishConfig": {
        "access": "public"
    }
}